{
  "name": "inversify-generator",
  "version": "0.1.3",
  "description": "IoC Boilerplate generator for dependency injection",
  "bin": {
    "inversify-generator": "./bin.js"
  },
  "files": [
    "bin.js",
    "generator",
    "decorators",
    "utils",
    "**/*.d.ts",
    "dist"
  ],
  "scripts": {
    "start": "npm run build && node build/index.js",
    "prepare": "husky install",
    "build": "preconstruct build",
    "watch": "preconstruct watch",
    "lint": "eslint --fix --ext js,ts,tsx src",
    "check-types": "tsc --project tsconfig.json",
    "dev": "preconstruct watch",
    "release": "commit-and-tag-version"
  },
  "author": "Mr.Mil√∫",
  "license": "MIT",
  "dependencies": {
    "chalk": "~4.1.2",
    "ts-morph": "~19.0.0",
    "chokidar": "~3.5.3",
    "yargs": "~17.7.2"
  },
  "peerDependencies": {
    "inversify": "~6.0.1"
  },
  "devDependencies": {
    "@babel/core": "~7.22.9",
    "@babel/preset-typescript": "~7.22.5",
    "@commitlint/cli": "~17.6.7",
    "@commitlint/config-conventional": "~17.6.7",
    "@commitlint/prompt-cli": "~17.6.7",
    "@preconstruct/cli": "~2.8.1",
    "@types/node": "~20.4.2",
    "@types/yargs": "~17.0.24",
    "@typescript-eslint/eslint-plugin": "~6.1.0",
    "@typescript-eslint/parser": "~6.1.0",
    "chalk": "~4.1.2",
    "chokidar": "~3.5.3",
    "commit-and-tag-version": "~11.2.2",
    "eslint": "~8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-unused-imports": "~3.0.0",
    "husky": "~8.0.3",
    "inversify": "~6.0.1",
    "lint-staged": "~13.2.3",
    "nodemon": "~3.0.1",
    "prettier": "~3.0.0",
    "rimraf": "~5.0.1",
    "ts-morph": "~19.0.0",
    "ts-node": "~10.9.1",
    "typescript": "~5.1.6",
    "yargs": "~17.7.2"
  },
  "preconstruct": {
    "entrypoints": [
      "./generator.ts",
      "./utils.ts",
      "./decorators.ts"
    ]
  }
}
