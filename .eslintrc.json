{
  "plugins": ["@typescript-eslint", "unused-imports"],
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": {
    "unused-imports/no-unused-imports": "error",
    "@typescript-eslint/consistent-type-imports": "error",
    "@typescript-eslint/array-type": [
      "error",
      {
        "default": "generic",
        "readonly": "generic"
      }
    ]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
